# OpenAI Configuration (REQUIRED)
OPENAI_API_KEY=your-openai-api-key-here

# Vanna AI Configuration
# Get your API key from https://app.vanna.ai
VANNA_API_KEY=your-vanna-api-key-here
VANNA_USER_EMAIL=your-email@example.com
VANNA_AGENT_ID=your-agent-id

# Vanna Question Prefixes (optional - defaults shown)
# These prefixes help route questions to the correct Vanna agent
VANNA_STRUCTURED_PREFIX=hosted app
VANNA_EXPLORATORY_PREFIX=hosted app

# Twilio WhatsApp Alerts (OPTIONAL)
# Set to "true" to enable WhatsApp alerts via Twilio
TWILIO_ENABLED=false

# Twilio Credentials (only needed if TWILIO_ENABLED=true)
# Get from https://www.twilio.com/console
TWILIO_ACCOUNT_SID=your-account-sid
TWILIO_AUTH_TOKEN=your-auth-token

# WhatsApp Numbers (only needed if TWILIO_ENABLED=true)
# Format: whatsapp:+1234567890 (include country code)
TWILIO_WHATSAPP_FROM=whatsapp:+14155238886
TWILIO_WHATSAPP_TO=whatsapp:+1234567890
